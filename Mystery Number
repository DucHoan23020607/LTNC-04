#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   
        int n;
         cin >> n;
        int arr1[n];
        int arr2[n+1];
        int sum1 = 0, sum2 = 0;
        for(int i = 0; i < n; i++)
        {
                cin >> arr1[i];
                sum1 += arr1[i];
        }
        for (int i = 0; i < n+1; i++)
        {
                cin >> arr2[i];
                sum2 += arr2[i];
        }
        cout << abs(sum2 - sum1);
    return 0;
}
